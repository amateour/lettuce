{
  "name": "@amateury/lettuce",
  "version": "2.0.0",
  "description": "It is a library that allows a rigorous validation of the data, according to a specific pattern (a scheme).",
  "main": "build/index.js",
  "module": "build/index.es.mjs",
  "umd:main": "build/index.umd.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "mocha",
    "coverage:report": "nyc npm test && nyc report --reporter=lcov",
    "coveralls": "npm run coverage:report  && coveralls < coverage/lcov.info",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "rollup -c --compact",
    "watch": "node_modules/.bin/webpack --config webpack.config.js --mode=development -w",
    "prepublish": "npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amateury/lettuce.git"
  },
  "bugs": {
    "url": "https://github.com/amateury/lettuce/issues"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "lettuce",
    "validator",
    "validation",
    "api",
    "argument",
    "amateur"
  ],
  "author": "Brayan Salgado",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
