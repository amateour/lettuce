{
  "name": "@amateury/lettuce",
  "version": "2.0.0-alfa",
  "description": "It is a library that allows a rigorous validation of the data, according to a specific pattern (a scheme).",
  "main": "build/index.js",
  "module": "build/index.es.mjs",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage --detectOpenHandles && coveralls < coverage/lcov.info",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "build": "npx webpack --config webpack.config.js --mode=production",
    "build:dev": "npx webpack --config webpack.config.js --mode=development",
    "watch": "node_modules/.bin/webpack --config webpack.config.js --mode=development -w",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amateury/lettuce.git"
  },
  "bugs": {
    "url": "https://github.com/amateury/lettuce/issues"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "lettuce",
    "validator",
    "validation",
    "api",
    "argument",
    "amateur"
  ],
  "author": "Brayan Salgado",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@jest/types": "^28.1.1",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
